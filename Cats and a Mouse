#include<stdio.h>
int main()
{
    int i,c1,c2,m,q,l,k;
    scanf("%d",&q);
    for(i=0;i<q;i++)
    {
        scanf("%d %d %d",&c1,&c2,&m);
        l=m-c1;
        k=m-c2;
        if(l<0)
        {
            l=l*(-1);
        }
        if(k<0)
        {
            k=k*-1;
        }
        if(l>k)
        {
            printf("Cat B");
        }
        else if(l<k)
        {
            printf("Cat A");
        }
        else
        {
            printf("Mouse C");
        }
        printf("\n");
        
    }
    return 0;
}

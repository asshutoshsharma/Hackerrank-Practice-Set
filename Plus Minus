#include<stdio.h>
int main()
{
    int n,i,p=0,n=0,z=0;
    float an=0.0,ap=0.0,az=0.0;
    int number;
    scanf("%d", &n);
    for(i=0;i<n;i++)
    {
      scanf("%d",&number);
       if(number>0)
            p+=1;
        else if(number < 0)
            n+=1;
       else
           z+=1;
    }
    ap=(float)p/n;
    an=(float)n/n;
    az=(float)z/n;
    printf("%.6f\n%.6f\n%.6f\n",ap,an,az);
    return 0;
}

